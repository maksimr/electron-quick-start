{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "npm-run-all --parallel electron webpack-dev",
    "test": "electron-mocha --renderer --require babel-register tests/*.js",
    "test:watch": "nodemon --watch *.js --watch tests/**/*.js --exec 'npm run -s test'",
    "electron": "electron .",
    "build": "NODE_ENV='production' webpack",
    "webpack-dev": "webpack-dev-server"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "electron": "~1.6.2",
    "electron-mocha": "^3.4.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "4.0.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-node-externals": "^1.5.4",
    "write-file-webpack-plugin": "^4.0.2"
  }
}
